                {% for review in item.right_half_edit_img %}
                  <div class="p-3 shadow bg-primary d-inline-block rounded-4 shadow-lg text-center" style="
                      background: url({{ site.theme.link }}/assets/images/pattern/02.png) no-repeat center
                      center;
                      background-size: cover;
                    ">
                    <!-- Info -->
                    <h5 class="text-white mb-0">
                      {{ review.average_review }}
                    </h5>
                    {% set rating = review.average_review | round(0, 'floor') %}
                    {% if rating > 1 %}
                      {% set full_stars = rating | round(0, 'floor') %}
                      {% set half_star = (rating - full_stars) >= 0.5 %}

                      <ul class="list-inline mb-2">
                        {% for i in 1..full_stars %}
                          <li class="list-inline-item me-0 small">
                            <i class="fas fa-star text-warning"></i>
                          </li>
                        {% endfor %}
                        {% if half_star %}
                          <li class="list-inline-item me-0 small">
                            <i class="fas fa-star-half-alt text-warning"></i>
                          </li>
                        {% endif %}
                      </ul>
                      <p class="text-white mb-0">
                        {{ review.total_based_review }}
                      </p>
                    {% endif %}
                  </div>
                {% endfor %}
              </div>

              <!-- Review -->
              <div class="col-md-5 mt-n6 mb-0 mb-md-5">
                <div class="bg-body shadow text-center p-0 rounded-3 position-relative mb-5 mb-md-0">
                  {% if image_three is not empty %}
                    <figure class="figure">
                      <img src="{{ image_three.src | resize(283,189) }}" class="figure-img img-fluid rounded" alt="{{ image_three.alt }}" srcset="{{ image_three.src }}" />
                      <figcaption class="figure-caption p-4 py-0">
                        <strong>
                          {{ image_three.alt }}
                        </strong>
                      </figcaption>
                    </figure>
                  {% endif %}
                </div>
              </div>
            </div>
          </div>
        {% endif %}
      {% endfor %}
