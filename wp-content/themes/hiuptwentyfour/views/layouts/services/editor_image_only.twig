<section class="bg-light">
    <div class="container pt-5 mt-2 mt-md-4 mt-lg-5">
        <div class="row pt-xl-3">
            {% for item in section.left_half_edit_img_only %}
                <div class="col-md-5 text-center text-md-start pb-5">
                    {{ item.wysiwyg_editor |raw |wpautop }}

                    {% set additional_img = get_image(item.additional_img) %}

                    {% if additional_img %}
                        <div class="my-2 mb-5">
                            <img src="{{ additional_img.src | resize(181,175) }}" alt="{{ additional_img.alt }}" />
                        </div>
                    {% endif %}

                    {% if item.button_url.url %}
                        <a href="{{ item.button_url.url }}" class="btn btn-primary-soft mb-0" {% if item.button_url.blank %} target="_blank" {% endif %}>
                            {{ item.button_url.anchor }}
                            <i class="bi bi-arrow-down-short ms-2"></i>
                        </a>
                    {% endif %}

                </div>
            {% endfor %}

            {% set thumbnail = get_image(section.thumbnail) %}

